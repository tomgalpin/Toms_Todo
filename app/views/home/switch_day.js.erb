var day = "<%= @selected_day_of_week %>"
var previous = "<%= @previous_day %>"



$("#" + day).empty().append('<%= j render :partial => "big_list", :locals => { :day_of_week => @selected_day_of_week.capitalize } %>')



$("#" + previous).empty().append('<%= j render :partial => "small_list", :locals => { :day_of_week => @previous_day.capitalize } %>')